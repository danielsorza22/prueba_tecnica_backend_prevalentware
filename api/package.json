{
  "name": "api",
  "version": "2.0.0",
  "main": "index.js",
  "license": "MIT",
  "exports": "./dist/index.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "generate_build": "cosmo && npx prisma generate && prettier --config .prettierrc prisma/generated/**/*.ts --write && next build",
    "cosmo": "cosmo && prettier --config .prettierrc prisma/generated/**/*.ts --write",
    "cosmo:deploy": "bunx prisma generate && cosmo",
    "generate": "npx prisma generate && bun run cosmo",
    "compile": "rimraf ./dist && tsc",
    "build": "esbuild index.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/index.js",
    "start": "ts-node dev.ts",
    "migration": "ts-node prisma/runMigration.ts",
    "start:prod": "node dist/index.js",
    "dev": "bun --hot dev.ts",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint:fix": "eslint \"./src/**/*.ts\" --fix"
  },
  "dependencies": {
    "@as-integrations/aws-lambda": "^3.1.0",
    "@aws-sdk/client-s3": "^3.556.0",
    "@aws-sdk/client-secrets-manager": "^3.556.0",
    "@aws-sdk/s3-request-presigner": "^3.556.0",
    "@prevalentware/prisma-cosmo": "^0.1.33",
    "@prisma/client": "^5.13.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "exceljs": "^4.4.0",
    "graphql-tag": "^2.12.6",
    "nanoid": "^5.0.7",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.13"
  },
  "devDependencies": {
    "@apollo/server": "^4.10.4",
    "@eslint/js": "^9.3.0",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.14",
    "dotenv": "^16.4.5",
    "esbuild": "^0.20.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fs": "^0.0.1-security",
    "graphql": "^16.8.1",
    "prisma": "^5.13.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
  }
}
